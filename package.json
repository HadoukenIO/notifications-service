    "url": "https://github.com/openfin/Notification-Service/issues"
  },
  "homepage": "https://github.com/openfin/Notification-Service#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/cucumber": "^4.0.1",
    "@types/jest": "^22.2.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^9.4.6",
    "@types/openfin": "^17.0.4",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "cucumber": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "fs": "0.0.1-security",
    "gts": "^0.5.4",
    "jest": "^22.4.2",
    "jquery": "^3.3.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.11.1",
    "react-dom": "^16.2.0",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.3",
    "ts-jest": "^22.4.1",
    "ts-loader": "^3.5.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "url-loader": "^1.0.1",
    "webpack": "3.10.0",
    "webpack-cli": "^2.0.13",
    "express": "^4.16.2",
    "moment": "^2.22.1",
    "npm": "^5.7.1",
    "openfin-launcher": "^1.4.2",
    "path": "^0.12.7",
    "react": "^16.2.0"
  },
  "pre-commit": [
    "lint"
  ],
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.(ts|tsx)"
    ]
  }
