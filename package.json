    "@types/cucumber": "^4.0.1",
    "@types/jest": "^22.2.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^9.4.6",
    "@types/openfin": "^17.0.4",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "copy-webpack-plugin": "^4.5.1",
