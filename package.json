{
  "name": "openfin-notifications",
  "version": "0.0.3",
  "description": "",
  "main": "module_dist/client/index.js",
  "types": "module_dist/client/index.d.ts",
  "scripts": {
    "test": "jest",
    "lint": "tslint --project . -t codeFrame",
    "lint:fix": "tslint --project . -t codeFrame --fix",
    "clean": "gts clean",
    "webpack": "webpack",
    "stage": "cp -R ./src/ui ./dist/ && cp -R ./src/demo ./dist/ && cp ./src/app.json ./dist/ && cp ./src/index.html ./dist/",
    "build": "npm run webpack && npm run stage",
    "deploy": "node deploy.js",
    "start": "npm run start:dev",
    "start:dev": "npm run build:demo && npm run serve:dev",
    "start:prod": "npm run build && npm run serve:prod",
    "serve": "npm run serve:dev",
    "serve:dev": "cross-env NODE_ENV=dev node server.js",
    "serve:prod": "cross-env NODE_ENV=prod node server.js",
    "prepare": "tsc --outDir module_dist src/client/index.ts --lib DOM,ES6 --declaration --target ES2017 --module commonjs",
    "build:demo": "npm run build && cd ./src/demo && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openfin/Notification-Service.git"
  },
  "author": "",
